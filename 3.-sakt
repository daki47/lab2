void print_by_year(film *f, int n, int year)
{
    for(int i=0;i<n;i++)
    {
        if(f[i].get_year()==year)
            f[i].print_info();
    }
}

int main() {
 	int n;
 	cin >> n;
 	//declare array of n film objects
film f[n];
 	for(int i = 0; i < n; ++i) {
 		char name[100];
 		char director[50];
 		char genre[50];
 		int year;
 		cin >> name;
 		cin >> director;
 		cin >> genre;
 		cin >> year;
 		f[i].setname(name);
 		f[i].setdirector(director);
 		f[i].setgenre(genre);
 		f[i].setyear(year);

 	}
    int curr_year;
 	cin >> curr_year;
 	print_by_year(f, n, curr_year);


 	return 0;
 }








